    "scripts": {
        "dev": "cross-env NODE_ENV=development vite ",
        "build": "cross-env NODE_ENV=production run-p build-only",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
